<h1>Listing administrators</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @admins.each do |admin| %>
  <tr>
    <td><%=h admin.name %></td>
    <td><%= link_to 'Show', "#{admin.id}" %></td>
    <td><%= link_to 'Edit', edit_administrator_path(admin)%></td>
    <td><%= link_to 'Destroy', admin, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New administrator', new_administrator_path %>
